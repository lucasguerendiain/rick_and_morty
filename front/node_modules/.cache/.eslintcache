alert('No hay personajes con ese ID');\n      }\n    })\n    .catch((error) => {\n      console.log(error.message);\n    })\n  }\n  const onClose = (id) => {\n    setCharacters(characters.filter((char) => char.id !== id))\n  }\n\n  return (\n    <div className='App' style={{ padding: '25px' }}>\n      <Nav onSearch={onSearch} logout={logout}/>\n      <Routes>\n        <Route path=\"/\" element={<Form login={login}/>}>Form</Route>\n        <Route path=\"/home\" element={<Cards characters={characters} onClose={onClose}/>}>Home</Route>\n        <Route path=\"/about\" element={<About/>}>About</Route>\n        <Route path=\"/favorites\" element={<Favorites/>}>Favorites</Route>\n        <Route path=\"/detail/:detailId\" element={<Detail/>}>CardDetail</Route>\n      </Routes>\n    </div>\n  )\n}\n\nexport default App\n","/home/guere/henry/rick_and_morty/front/src/redux/store/store.js",[],[],"/home/guere/henry/rick_and_morty/front/src/components/Cards.jsx",[],[],"/home/guere/henry/rick_and_morty/front/src/components/Nav/Nav.jsx",[],[],"/home/guere/henry/rick_and_morty/front/src/components/Favorites.jsx",["76"],[],"import styles from \"./Favorites.module.css\";\nimport Card from \"./Card/Card\";\nimport { connect } from \"react-redux\";\nimport { useDispatch } from \"react-redux\";\nimport { orderCards, filterCards, getFavorites } from \"../redux/actions/actions\";\nimport { useEffect } from \"react\";\n\nexport function Favorites({ myFavorites }) {\n    const dispatch = useDispatch();\n\n    const handleSort = (event) => {\n        dispatch(orderCards(event.target.value));\n    }\n\n    const handleFilter = (event) => {\n        dispatch(filterCards(event.target.value));\n    }\n\n    useEffect(() => {\n        dispatch(getFavorites());\n    }, []);\n\n    return (\n        <div className={styles.FavDiv}>\n            <select name=\"ordenar\" onChange={handleSort}>\n                <option value=\"Ascendente\">Ascendente</option>\n                <option value=\"Descendente\">Descendente</option>\n            </select>\n            <select name=\"filtrar\" onChange={handleFilter}>\n                <option value=\"allCharacters\">All characters</option>\n                <option value=\"Male\">Male</option>\n                <option value=\"Female\">Female</option>\n                <option value=\"Genderless\">Genderless</option>\n                <option value=\"Unknown\">Unknown</option>\n            </select>\n            <div className={styles.FavList}>\n                {myFavorites && myFavorites.map((char, index) => {\n                return <Card \n                    name={char.name} \n                    species={char.species} \n                    gender={char.gender} \n                    image={char.image}\n                    event={\"\"} \n                    id={char.id}\n                key={\"f\" + char.id+ index}/>\n            })}\n            </div>\n        </div>\n        );\n};\n\nexport function mapStateToProps(state) {\n    return {\n        myFavorites: state.myFavorites\n    };\n};\n\nexport default connect(mapStateToProps, null)(Favorites);","/home/guere/henry/rick_and_morty/front/src/components/Detail/Detail.jsx",[],[],"/home/guere/henry/rick_and_morty/front/src/components/Form/Form.jsx",[],[],"/home/guere/henry/rick_and_morty/front/src/components/About/About.jsx",[],[],"/home/guere/henry/rick_and_morty/front/src/redux/reducer/reducer.js",[],[],"/home/guere/henry/rick_and_morty/front/src/components/Form/validation.jsx",[],[],"/home/guere/henry/rick_and_morty/front/src/components/Nav/SearchBar.jsx",[],[],"/home/guere/henry/rick_and_morty/front/src/redux/actions/actions.js",[],[],"/home/guere/henry/rick_and_morty/front/src/components/Card/Card.jsx",["77"],[],"import { Link } from \"react-router-dom\";\nimport styles from \"./Card.module.css\";\nimport { addFavorite, deleteFavorite } from \"../../redux/actions/actions\";\nimport { useEffect, useState } from \"react\";\nimport { connect } from \"react-redux\";\n\nexport function Card(props) {\n   const {name, species, gender, image, event, id, deleteFavorite, myFavorites} = props\n   const [isFav, setIsFav] = useState(false)\n\n   const handleFavorite = () => {\n      if (isFav) {\n         setIsFav(false);\n         deleteFavorite(id);\n      }\n      else {\n         setIsFav(true);\n         const {addFavorite, deleteFavorite, myFavorites, event, ...aux} = props; \n         addFavorite(aux);\n      }\n   }\n\n   useEffect(() => {\n      myFavorites.forEach((fav) => {\n         if (fav.id === id) {\n            setIsFav(true);\n         }\n      });\n   }, [myFavorites]);\n\n   const handleGender = () => {\n      switch(gender){\n         case \"Male\":\n            return styles.NombreMale;\n         case \"Female\":\n            return styles.NombreFemale;\n         case \"Genderless\":\n            return styles.NombreGenderless;\n         case \"unknown\":\n            return styles.NombreUnknown;\n         default:\n            return \"\";\n      }\n   }\n\n   return (\n      <div className={styles.CardDiv}>\n         {event ? (<button className={styles.Button} onClick={event}>X</button>) : null}\n         <Link to={`/detail/${id}`} className={styles.link}>\n            <h2 className={handleGender()}>{name}</h2>\n         </Link>\n         <h2 className={styles.Specie}>{species}</h2>\n         <h2 className={styles.Gender}>{gender}</h2>\n         <img src={image} alt=\"\" className={event ? styles.ImagenGira : styles.ImagenGirant}/>\n         {isFav ? (\n            <button onClick={handleFavorite} className={styles.FavButtonOn}>❤️</button>\n                  ) : (\n            <button onClick={handleFavorite} className={styles.FavButtonOff}>🤍</button>)\n         }\n      </div>\n   );\n}\n\nexport function mapDispatchToProps(dispatch) {\n   return {\n      addFavorite:   (char) => {dispatch(addFavorite(char))},\n      deleteFavorite: (id) => {dispatch(deleteFavorite(id))}\n   }\n};\n\nexport function mapStateToProps(state) {\n   return {\n      myFavorites: state.myFavorites\n   };\n};\n\nexport default connect(mapStateToProps, mapDispatchToProps)(Card);",{"ruleId":"78","severity":1,"message":"79","line":35,"column":6,"nodeType":"80","endLine":35,"endColumn":14,"suggestions":"81"},{"ruleId":"78","severity":1,"message":"82","line":21,"column":8,"nodeType":"80","endLine":21,"endColumn":10,"suggestions":"83"},{"ruleId":"78","severity":1,"message":"84","line":29,"column":7,"nodeType":"80","endLine":29,"endColumn":20,"suggestions":"85"},"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'navigate'. Either include it or remove the dependency array.","ArrayExpression",["86"],"React Hook useEffect has a missing dependency: 'dispatch'. Either include it or remove the dependency array.",["87"],"React Hook useEffect has a missing dependency: 'id'. Either include it or remove the dependency array.",["88"],{"desc":"89","fix":"90"},{"desc":"91","fix":"92"},{"desc":"93","fix":"94"},"Update the dependencies array to be: [access, navigate]",{"range":"95","text":"96"},"Update the dependencies array to be: [dispatch]",{"range":"97","text":"98"},"Update the dependencies array to be: [id, myFavorites]",{"range":"99","text":"100"},[1017,1025],"[access, navigate]",[611,613],"[dispatch]",[826,839],"[id, myFavorites]"]